<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="subway.v5">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Login.GlobalApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Subway\nManager"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.CardActivity"></activity>
        <activity android:name=".Login.CheckLoginActivity"></activity>
        <activity android:name=".Login.UserDataViewActivity"></activity>
        <activity android:name=".TTSActivity"></activity>
        <!-- 카카오톡 메타데이터 -->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
    </application>

</manifest>